    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "preLaunchTask": "build",
            "name": "Launch help",
            "outFiles": ["${workspaceRoot}/js/**"],
            "program": "${workspaceRoot}/src/index.ts",
            "sourceMaps": true,
            "args": [
                "--help"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "preLaunchTask": "build",
            "name": "Launch list",
            "outFiles": ["${workspaceRoot}/js/**"],
            "program": "${workspaceRoot}/src/index.ts",
            "sourceMaps": true,
            "args": [
                "list"
            ],
            "cwd": "${workspaceRoot}/examples/test"
        },
        {
            "type": "node",
            "request": "launch",
            "preLaunchTask": "build",
            "name": "Launch generate",
            "outFiles": ["${workspaceRoot}/js/**"],
            "program": "${workspaceRoot}/src/index.ts",
            "sourceMaps": true,
            "args": [
                "generate", "--clear", "--out", "./../output", "--modules", "default|services|settings", "RfidCloud"
            ],
            "cwd": "${workspaceRoot}/examples/test"
        },
        {
            "type": "node",
            "request": "launch",
            "preLaunchTask": "build",
            "name": "Launch init",
            "outFiles": ["${workspaceRoot}/js/**"],
            "program": "${workspaceRoot}/src/index.ts",
            "sourceMaps": true,
            "args": [
                "init"
            ],
            "cwd": "${workspaceRoot}/examples/test"
        },
        {
            "type": "node",
            "request": "launch",
            "preLaunchTask": "build",
            "name": "Launch module",
            "outFiles": ["${workspaceRoot}/js/**"],
            "program": "${workspaceRoot}/src/index.ts",
            "sourceMaps": true,
            "args": [
                "module",
                "testmodule"
            ],
            "cwd": "${workspaceRoot}/examples/test"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "An den Port anf√ºgen",
            "address": "localhost",
            "port": 5858
        }
    ]
}